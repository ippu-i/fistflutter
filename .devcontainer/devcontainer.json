{
	"customizations": {
        "vscode": {
            "extensions": [
                "Dart-Code.dart-code",
                "Dart-Code.flutter"
            ],
            "settings": {
              "dart.flutterSdkPath": "/workspaces/flutter/bin/"
            }
        }
    },
    "containerEnv": {
      "FLUTTER_HOME": "/workspaces/flutter/bin/"
    },
    "remoteEnv": {
      "PATH": "${containerEnv:PATH}:${containerEnv:FLUTTER_HOME}:/root/.pub-cache/bin"
    }
}
